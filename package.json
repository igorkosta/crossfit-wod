{
  "name": "crossfit-wod",
  "version": "0.0.5",
  "description": "crossfit-wod will log a random workout of the day from https://crossfit.com/workout/ to your console",
  "main": "lib/index.js",
  "bin": {
    "crossfit-wod": "./lib/index.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/lab -a code -t 100 -L -r console -o stdout -r lcov -o coverage/lcov.info",
    "test-html": "mkdir -p coverage && ./node_modules/.bin/lab -a code -t 100 -L -r html > coverage/index.html"
  },
  "homepage": "https://github.com/igorkosta/crossfit-wod",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorkosta/crossfit-wod"
  },
  "bugs": {
    "url": "https://github.com/igorkosta/crossfit-wod/issues"
  },
  "keywords": [
    "crossfit",
    "workout",
    "wod",
    "thrusters",
    "wall balls",
    "deadlift",
    "pull-ups",
    "handstand push-ups",
    "cleans",
    "jerks"
  ],
  "dependencies": {
    "cheerio": "^0.20.0",
    "moment": "^2.13.0",
    "request": "^2.72.0"
  },
  "author": "igorkosta",
  "license": "MIT",
  "devDependencies": {
    "code": "^4.0.0",
    "lab": "^11.1.0",
    "request": "^2.75.0"
  }
}
